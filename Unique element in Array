#include<iostream>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++)
    {
        
        cin>>arr[i];
    }
    int i;
    while(i<n)
    {
    
    if(arr[i]!=arr[i+1])
    {
        cout<<"The number is unique "<<endl;
    }
    else
    {
        cout<<"The number is not unique"<<endl;
    }
    i++;
}
}
