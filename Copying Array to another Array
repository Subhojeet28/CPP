#include<iostream>
using namespace std;

int main()
{
    
    int n;
    cin>>n;
    
    int arr[n];
    
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(int i=0;i<n;i++)
    {
        
        cout<<arr[i]<<" ";
    }
    cout<<endl;
    for(int j=0;j<n;j++)
    {
        int i;
        arr[i]=arr[j];
        cout<<arr[j]<<" ";
    }
}
